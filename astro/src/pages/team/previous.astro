---
import Layout from '../../layouts/Layout.astro';
import ThemedSection from '../../components/ThemedSection.astro';

const title = 'Previous Contributors';
const heading = title;

const crumbs = [{
  name: 'Home', href: '/',
}, {
  name: 'Team', href: '/team',
}];
---

<Layout title={title} crumbs={crumbs} heading={heading}>
  <ThemedSection style="secondary">
    <div class="col-xxl-9 pb-3">
      <p class="lead">We sincerely appreciate the time and energy that each person listed below has contributed to our mission.</p>
    </div>
    <div class="card mb-5 shadow team-card">
      <h2 class="card-header text-primary-emphasis">Board Members</h2>
      <div class="card-body">
        <ul>
          <li>Tammi Smith (Board Secretary, 2019-2021)</li>
          <li>Ben Wilson (Board President, 2017-2019)</li>
          <li>Cham L. (Board Member, 2021-2023)</li>
        </ul>
      </div>
    </div>
    <div class="card mb-5 shadow team-card">
      <h2 class="card-header text-primary-emphasis">Leadership Team</h2>
      <div class="card-body">
        <ul>
          <li>Abdullah S. (Director of Donor Relations, 2020-2021)</li>
          <li>Marjana C. (Loca11y Project Manager, 2022)</li>
        </ul>
      </div>
    </div>
    <div class="card mb-5 shadow team-card">
      <h2 class="card-header text-primary-emphasis">Contributors</h2>
      <div class="card-body">
        <ul class="list-inline mx-3">
          <li class="list-inline-item">Brian Chu</li>
