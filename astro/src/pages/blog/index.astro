---
import { getBlogCatalog, type BlogCatalog } from "../../lib/blog";

const catalog: BlogCatalog = await getBlogCatalog();
const crumbs = [
  {
    name: "Home",
    href: "/",
  },
];

import BlogLayout from "../../layouts/BlogLayout.astro";
---

<BlogLayout
  title="Blog"
  blog={catalog.recent}
  catalog={catalog}
  crumbs={crumbs}
  sidebar={true}
/>
